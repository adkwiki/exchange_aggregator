{
  "name": "exchange_aggregator",
  "version": "1.0.0",
  "description": "ADK listing exchange info aggregatr",
  "main": "dist/server.js",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "build-ts": "tsc",
    "postinstall": "yarn run build-ts",
    "start": "yarn run serve",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w"
  },
  "repository": "git+https://github.com/adkwiki/exchange_aggregator.git",
  "author": "m2Lower",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adkwiki/exchange_aggregator/issues"
  },
  "homepage": "https://github.com/adkwiki/exchange_aggregator#readme",
  "engines": {
    "node": "12.9.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.5",
    "axios": "^0.19.0",
    "cloudscraper": "^4.3.0",
    "express": "^4.17.1",
    "pg": "^7.12.1"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-node-externals": "^1.7.2"
  }
}
